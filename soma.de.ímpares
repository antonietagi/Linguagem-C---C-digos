#include <stdio.h>

int main(){
    int n;
    int i;
    int soma;
    printf("Digite um n√∫mero:");
    scanf("%d", &n);
    
   
    for (i=1; i<=n ;i++) {
     
     if(i % 2 != 0){  
      soma += i;    
      printf("%d ", i);
     
    
      } 
    } 
     printf("\nsoma=%d", soma);   
    return 0;
}
