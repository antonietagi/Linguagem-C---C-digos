#include <stdio.h>

int main(){
   int i;
   
   printf("FOR");
   for (i=1;i<=10;i++){
        printf(" %d ", i);
   }


    printf("\nAo contrÃ¡rio =):");
    for(i=10;i>=1;i--){
        printf(" %d ", i);
    }
    
    printf("WHILE");
    while(i<=10){
    printf(" %d ", i);
    i++;
    }
    
    while(i>=1){
        printf(" %d ", i);
        i--;
    }
    
    
    
}
