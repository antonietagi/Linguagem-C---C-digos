#include <stdio.h>

int main()
{
   int a, b;
   
   printf("Digite dois números:");
   scanf("%d %d", &a, &b);
   
   
   printf("\nValores Originais ");
   printf("A=%d, B=%d", a, b); 
   
   int temp = a;
   a = b;
   b = temp;
   
   printf("\nValores após troca ");
   printf("A=%d, B=%d", a, b);   
   
   
   return 0;
}
